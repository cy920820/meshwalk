{
  "name": "meshwalk",
  "version": "0.0.0",
  "author": "Yomotsu",
  "license": "MIT",
  "main": "dist/meshwalk.js",
  "repository": "yomotsu/meshwalk.js",
  "jsnext:main": "dist/meshwalk.module.js",
  "module": "dist/meshwalk.module.js",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/runtime": "^7.20.13",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "rollup": "^3.12.0",
    "terser": "^5.16.1"
  },
  "scripts": {
    "dev": "rollup --config --watch",
    "build": "rollup --config && terser dist/meshwalk.js -o dist/meshwalk.min.js --comments '/^!/'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "browserslist": [
    "ie >= 11",
    "safari >= 9",
    "ios >= 9",
    "Android >= 5"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": false
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "keywords": [
    "three",
    "three.js",
    "tps",
    "game",
    "collision",
    "physics"
  ],
  "dependencies": {
    "camera-controls": "^2.0.1"
  }
}
